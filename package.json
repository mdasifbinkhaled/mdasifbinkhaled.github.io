{
  "name": "mdasifbinkhaled.github.io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production npm run build && next export",
    "postbuild": "sh -c 'mkdir -p out && touch out/.nojekyll'",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "pre-commit": "npm run lint && npm run test:run && tsc -p tsconfig.json --noEmit",
    "validate": "npm run lint && npm run test:run && tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf .next out dist coverage .turbo",
    "reset": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "analyze": "npm run build:analyze && npx @next/bundle-analyzer .next"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.5.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.9.6",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.5.3",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.16",
    "tsc-files": "^1.1.4",
    "typescript": "^5.7.2",
    "vitest": "^3.1.4"
  }
}
