{
  "name": "mdasifbinkhaled.github.io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production npm run build && sh -c 'mkdir -p out && touch out/.nojekyll'",
    "postbuild": "echo \"(no-op; export handled by output: export in next.config.ts)\"",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "next lint",
    "pre-commit": "tsc --noEmit",
    "validate": "npm run lint && npm run test:run && tsc -p tsconfig.json --noEmit",
    "deploy": "npm run build:production && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.x",
    "@testing-library/react": "^16.x",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.x",
    "autoprefixer": "^10.4.20",
    "commitlint": "^19.8.1",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.3",
    "jsdom": "^25.x",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.x",
    "vitest": "^3.2.4"
  }
}
